		"in": [
			{"name": "id", "type": "uint32_t"}
		],
		"out": [
			{"name": "ias", "type": "struct ipc_app_state"}
		]
	},

	"system_get_clients": {
		"out": [
			{"name": "clients", "type": "struct ipc_client_list"}
		]
	},

	"system_set_primary_client": {
		"in": [
			{"name": "id", "type": "uint32_t"}
		]
	},

	"system_set_focused_client": {
		"in": [
			{"name": "id", "type": "uint32_t"}
		]
	},

	"system_toggle_io_client": {
		"in": [
			{"name": "id", "type": "uint32_t"}
		]
	},

	"system_toggle_io_device": {
		"in": [
			{"name": "id", "type": "uint32_t"}
